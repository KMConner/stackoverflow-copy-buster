{
    "name": "Stack Overflow Copy Blocker",
    "manifest_version": 2,
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png"
    },
    "background": {
        "scripts": [
            "bundle.js"
        ],
        "persistent": false
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions": [
        "webNavigation",
        "http://*/",
        "https://*/"
    ]
}